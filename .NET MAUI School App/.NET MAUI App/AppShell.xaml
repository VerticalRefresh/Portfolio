<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Barham_C971.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Barham_C971"
    Shell.FlyoutBehavior="Disabled"
    xmlns:pages="clr-namespace:Barham_C971.Pages"
    Title="Barham_C971">

    <ShellContent
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="MainPage" />
    <ShellContent
        ContentTemplate="{DataTemplate pages:Login}"
        Route="Login" />
    <ShellContent
        ContentTemplate="{DataTemplate pages:CoursePage}"
        Route="CoursePage"/>


</Shell>
